import java.util.Scanner;

public class Main {
	//첫째 줄에 (a+b)%c, 둘째 줄에 ((a%c) + (b%c))%c, 셋째 줄에 (a×b)%c, 넷째 줄에 ((a%c) × (b%c))%c를 출력한다.
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		sc.nextLine();
		int b = sc.nextInt();

		int c = Integer.parseInt(String.valueOf(b).substring(2, 3))*a;
		int d = Integer.parseInt(String.valueOf(b).substring(1, 2))*a;
	    int e = Integer.parseInt(String.valueOf(b).substring(0, 1))*a;
	    int f = c+10*d+100*e;
	    
	    System.out.println(c);
	    System.out.println(d);
	    System.out.println(e);
	    System.out.println(f);
				
	}

}
