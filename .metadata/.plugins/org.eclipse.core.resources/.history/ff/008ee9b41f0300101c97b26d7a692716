package data_structure;

public class ArrayStack {

	int size;
	int top;
	Object data[];
	
	public ArrayStack(int size){
		this.data = new Object[size];
		this.top = -1 ; //-1로 초기화
	}
	
	public boolean isEmpty() {
	    return top == -1;
	}
	
	public boolean isFull() {
		return size == top-1;
	}
	
	public void push(Object obj) {
		data[++top] = obj;
	}
	public Object pop() {
		return data[top--];
	}
	public Object peep() {
		return data[top];
	}
}
