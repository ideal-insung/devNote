package algorithm;

import data_structure.LinkedListGraph;

public class DFS_main {

	
	public static void main(String[] args) {
		LinkedListGraph lls = new LinkedListGraph(10);
		lls.addEdge(0, 1);
		lls.addEdge(0, 2);
		lls.addEdge(0, 3);
		lls.addEdge(1, 2);
		lls.addEdge(1, 4);
		lls.addEdge(2, 3);
		lls.addEdge(2, 4);
		lls.addEdge(2, 5);
		lls.addEdge(3, 5);
		lls.addEdge(4, 5);
		lls.addEdge(4, 8);
		lls.addEdge(5, 6);
		lls.addEdge(6, 7);
		lls.addEdge(6, 8);
		lls.addEdge(7, 8);
		lls.addEdge(9, 8);
		lls.display();
		
		//DFS dfs = new DFS();
		//dfs.dfs_stack(lls,0);
		
		
	}
}
