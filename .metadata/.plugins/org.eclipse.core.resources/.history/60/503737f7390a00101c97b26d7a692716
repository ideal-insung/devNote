package algorithm;

import java.util.Scanner;

/*
카드 N장중 M과 가장 가까운 수를 뽑는 
블랙잭 게임 

 */
public class BlackJack {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int M = sc.nextInt();
		int []arr = new int[N];
		for(int i=0; i<N;i++) {
			arr[i] = sc.nextInt();
		}
		
		for(int a : arr) {
			System.out.print(a);
		}
	}
}
