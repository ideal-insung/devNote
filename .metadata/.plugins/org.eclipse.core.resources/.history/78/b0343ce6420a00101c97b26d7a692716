package algorithm;

import java.util.Scanner;

/*
카드 N장중 M과 가장 가까운 수를 뽑는 
블랙잭 게임 

 */
public class BlackJack {
	public static int blackJack(int[]arr, int count, int target) {
		int sum = 0;
		if(arr == null) return -1;
		for(int i=0;i<count-2;i++) { //0,1,2
			for(int j=i+1; j< count-1; j++) { //1,2,3
				for(int z = j+1; z< count; z++) { //2,3,4인덱스
					if(arr[i]+arr[j]+arr[z] == target) {
						return sum = arr[i]+arr[j]+arr[z];
					}else {
						int tmp = arr[i]+arr[j]+arr[z];
						if(sum < tmp && tmp < target) {
							int tmp1 = tmp;
							if(tmp1 > tmp) {
								System.out.println(tmp1);
							}
						}
					}
				}
			}
		}
		return sum;
	}
