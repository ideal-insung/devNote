package algorithm;

public class fibo {

	public int fibo_sequence(int x) {
		int[] array = new int[101];
		if(x < 2) return x;
		else return  fibo_sequence(x-1) + fibo_sequence(x-2);
	}
	public static void main(String[] args) {
		
		fibo f = new fibo();
		for(int i=0; i<4;i++) {
			System.out.println(f.fibo_sequencei));
		}
		
	}
}
