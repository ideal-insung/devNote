public class back1018 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int M = sc.nextInt();
		char arr[][] = new char[N][M];
		
		for(int i=0; i< N; i++) {
			for(int j=0; j<M;j++) {
				arr[i][j] = sc.next().charAt(j);
			}
		}
		
		for(int i=0; i< N; i++) {
			for(int j=0; j<M;j++) {
				System.out.println(arr[i][j]);
			}
		}
		
	}
}
