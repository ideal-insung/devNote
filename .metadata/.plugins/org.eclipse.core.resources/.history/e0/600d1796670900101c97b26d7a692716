package algorithm;

public class MSTmain {

	
	public static void main(String[] args) {
		MST mst = new MST(7);
		for(int a : mst.prvNum) {
			System.out.print(a);
		}
		
		mst.union(1, 2);
		mst.union(2, 3);
		mst.union(3, 4);
		mst.union(5, 6);
		mst.union(5, 7);
		mst.union(6, 7);
		
		System.out.println(mst.isConnect(1, 6));
		mst.union(1, 5);
		System.out.println(mst.isConnect(1, 6));
	}
	
}
